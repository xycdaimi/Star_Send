/* pages/register/register.wxss */
.title{
  color: rgb(137, 214, 250);
  font-family: 'Pmzdqst';
  font-size: 45px;
  margin-top: 30%;
}
.block{
  height: 5vh;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-left: 8%;
}
.but{
  margin-left: 0px;
  height: 6vh;
}
.t{
  border-radius: 20rpx;
  background-color: rgb(110, 232, 253);
  color: rgba(179, 112, 243, 0.651);
  border: black 1px solid;
  height: 95%;
}
.inw{
  font-family: 'Pmzdqst';
  font-size: 32px;
  color: rgb(66, 255, 255);
}
.p{
  font-size: 20px;
}
.inp{
  width: 57%;
  height: 100%;
  border: rgb(242, 250, 250) 1px solid;
  border-right: none;
  border-top-left-radius: 10px;
  border-bottom-left-radius: 10px;
  margin: 0px auto;
  margin-left: 0px;
  margin-right: 0px;
  color: yellow;
  padding-left: 10px;
}
.placeholder-style{
  color: yellow;
}
.tu{
  margin: 0 auto;
  margin-left: 0px;
  height: 100%;
  width: 8%;
  border: rgb(242, 250, 250) 1px solid;
  border-top-right-radius: 10px;
  border-bottom-right-radius: 10px;
  border-left: none;
}
.tu image{
  margin-top: 10%;
  margin-left: 0px;
  margin-right: 0px;
  height: 80%;
  width: 80%;
}